@model IEnumerable<MVCEmpresa.Models.Sueldo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SueldoEmpleado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FormaDePago)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmpleadoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SueldoEmpleado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FormaDePago)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmpleadoNavigation.ClaveDeEmpleado)
            </td>
            <td>
                <a asp-action="Edit" class="btn btn-Warning" asp-route-id="@item.FakeId">Edit</a> |
                <a asp-action="Details" class="btn btn-primary" asp-route-id="@item.FakeId">Details</a> |
                <a asp-action="Delete" class="btn btn-danger" asp-route-id="@item.FakeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
